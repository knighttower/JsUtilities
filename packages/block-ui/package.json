{
  "name": "@knighttower/block-ui",
  "version": "1.10.0",
  "type": "module",
  "description": "A simple block ui plugin for Jquery or CashJs",
  "main": "dist/cjs/blockui.cjs",
  "module": "dist/esm/blockui.js",
  "unpkg": "dist/browser/blockui.js",
  "jsdelivr": "dist/browser/blockui.js",
  "browser": "dist/browser/blockui.js",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "exports": {
