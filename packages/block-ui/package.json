  },
  "keywords": [
    "utility",
    "library",
    "plugin",
    "URL",
    "javascript",
    "DOM",
    "functions",
    "helper",
    "knighttower",
    "blockui",
    "blockUI",
    "Jquery",
    "CashJs"
  ],
  "scripts": {
    "dev": "webpack --mode development",
    "watch": "webpack --mode development --watch",
    "test": "vitest"
  },
  "homepage": "https://github.com/knighttower/JsUtility",
  "repository": {
    "type": "git",
    "url": "git://github.com/knighttower/JsUtility.git",
    "directory": "packages/block-ui"
  }
}