{
  "name": "@knighttower/js-utility-functions",
  "version": "4.2.9",
  "type": "module",
  "main": "index.cjs",
  "module": "index.js",
  "jsdelivr": "dist/browser/Utility.js",
  "browser": "dist/browser/Utility.js",
  "license": "MIT",
  "description": "UtilityJs is a utility library that provides a collection of utility functions for various tasks. The library is designed to be easy to use and covers the most common use cases. ",
  "homepage": "https://github.com/knighttower/JsUtility",
  "author": "knighttower (https://github.com/knighttower)",
  "files": [
    "dist",
    "src",
    "nodeUtils",
    "index.js",
    "index.cjs"
  ],
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.cjs"
    },
    "./dist/*": "./dist/*",
    "./node/*": "./nodeUtils/*",
    "./nodeUtils/*": "./nodeUtils/*",
    "./nodeUtils": "./nodeUtils/index.cjs"
  },
  "bin": {
    "buildExports": "./nodeUtils/BuildExports.js",
    "bumpVersion": "./nodeUtils/BumpVersion.js"
  },
  "keywords": [
    "Utility",
    "Vue",
    "react",
    "library",
    "plugin",
    "URL",
    "Vue 2",
    "Vue 3",
    "javascript",
    "DOM",
    "functions",
    "helper",
    "knighttower",
    "js-utility-functions",
    "js-utility",
    "URL parser",
    "URL query",
    "Observer",
    "Proxy",
    "Element",
    "utilities",
    "node",
    "classes"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/knighttower/JsUtility.git"
  },
  "scripts": {
    "production": "npm run build",
    "build": "npm run webpack && npm run rollup && npm run buildExports && npm run bumpVersion",
    "bumpVersion": "node ./nodeUtils/BumpVersion.cjs --exe",
    "buildExports": "node ./nodeUtils/BuildExports.js --dir ./src && node ./nodeUtils/BuildExports.js --dir ./dist/cjs --type cjs",
    "rollup": "rollup -c ./nodeUtils/rollup.config.cjs",
    "webpack": "webpack --mode production --config ./nodeUtils/webpack.config.cjs",
    "dev": "webpack --mode development",
    "watch": "webpack --mode development --watch",
    "test": "npm run format && npm run eslint && vitest",
    "format": "prettier --config .prettierrc.json --write ./src",
    "format-check": "prettier --config .prettierrc.json --check ./src",
    "eslint": "eslint -c .eslintrc.json --fix ./src --ext .js,.cjs,.mjs",
    "minify": "node ./nodeUtils/Minify.js"
  },
  "dependencies": {
    "@knighttower/js-event-bus": "^3.3.0"
  }
}
