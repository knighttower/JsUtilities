{
  "name": "@knighttower/js-utility-functions",
  "version": "3.1.14",
  "type": "module",
  "main": "index.cjs",
  "module": "index.js",
  "jsdelivr": "dist/browser/Utility.js",
  "browser": "dist/browser/Utility.js",
  "license": "MIT",
  "description": "UtilityJs is a utility library that provides a collection of utility fungitgctions for various tasks. The library is designed to be easy to use and covers the most common use cases. ",
  "homepage": "https://github.com/knighttower/JsUtility",
  "author": "knighttower (https://github.com/knighttower)",
  "files": [
    "dist",
    "src",
    "nodeUtils",
    "index.js",
    "index.cjs"
  ],
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.cjs"
    },
    "./dist/*": "./dist/*"
  },
  "keywords": [
    "Utility",
    "Vue",
    "react",
    "library",
    "plugin",
    "URL",
    "Vue 2",
    "Vue 3",
    "javascript",
    "DOM",
    "functions",
    "helper",
    "knighttower",
    "js-utility-functions",
    "js-utility",
    "URL parser",
    "URL query",
    "Observer",
    "Proxy",
    "Element"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/knighttower/JsUtility.git"
  },
  "scripts": {
    "production": "npm run build",
    "build": "npm run webpack && npm run rollup && npm run buildExports && npm run bumpVersion",
    "bumpVersion": "node ./nodeUtils/BumpVersion.cjs",
    "buildExports": "node ./nodeUtils/BuildExports.js --dir ./src --cdir ./dist/cjs",
    "rollup": "rollup -c ./nodeUtils/rollup.config.cjs",
    "webpack": "webpack --mode production --config ./nodeUtils/webpack.config.cjs",
    "dev": "webpack --mode development",
    "watch": "webpack --mode development --watch",
    "test": "npm run format && npm run eslint && vitest",
    "format": "prettier --config .prettierrc.json --write ./src",
    "format-check": "prettier --config .prettierrc.json --check ./src",
    "eslint": "eslint -c .eslintrc.json --fix ./src --ext .js,.cjs,.mjs",
    "minify": "node ./nodeUtils/Minify.js"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/plugin-transform-object-assign": "^7.22.5",
    "@babel/preset-env": "^7.23.2",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-buble": "^1.0.3",
    "@rollup/plugin-commonjs": "^25.0.5",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "compression-webpack-plugin": "^10.0.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jsdom": "^22.1.0",
    "prettier": "^3.0.3",
    "pretty-quick": "^3.1.3",
    "rollup": "^4.0.2",
    "terser": "^5.21.0",
    "vitest": "^0.34.6",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}